  }

  @keyframes colorShift {
    0% { color: yellow; }
    25% { color: cyan; }
    50% { color: magenta; }
    75% { color: lime; }
    100% { color: yellow; }
  }

  .profile-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
    gap: 20px;
    max-width: 1000px;
    margin: 0 auto;
  }

  .profile-box {
    background: #111;
    border: 1px solid #333;
    color: white;
    padding: 15px;
    text-align: center;
  }

  .profile-box img {
